{
  "console": {
    "enable": true,
    "level": "FINE"
  },
  "files": [
    {
    "enable": true,
    "level": "DEBUG",
    "filename":"/Users/ledoankien/Go-Projects/src/restaurant/logs/router.log",
    "category": "SERVER",
    "pattern": "[%D %T] [%C] [%L] (%S) %M"
    },
    {
    "enable": false,
    "level": "DEBUG",
